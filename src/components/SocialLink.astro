---
// Components
import { Icon } from "astro-icon/components";

// Props
interface Props {
  name: string;
  url: string;
  icon: string;
}

const { name, url, icon } = Astro.props;
---

<li>
  <a
    href={url}
    target="_blank"
    class={name +
      " text-neutral-500 hover:text-neutral-900 dark:hover:text-white"}
  >
    <Icon name={icon} class="h-5 w-5" />
  </a>
</li>